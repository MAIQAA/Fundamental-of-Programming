#include<iostream>
#include<stdio.h>
using namespace std;
int main()
{
	int a[25]={1,2,6,3,8,22,99,6,5,8,3,2,1,3,66,4,8,5,9,2,4,0,32,65,37};
	cout<<"25 Elements of Array are : "<<endl;
	
	for(int i=1;i<25;i++)
	{
  cout<<a[i]<<" ";	
    }
    
    cout<<"\n"<<endl;
    
    int s;
    cout<<"Enter the Number you want to search : ";
    cin>>s;
    int count=0;
    for(int i=0;i<10;i++)
    {
        if(a[i]!='\0')
            count++;
    }
    cout<<"Elements in array are: "<<count;
}
