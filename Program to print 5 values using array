#include<iostream>
#include<conio.h>
using namespace std;
int main()
{
	int a[2][2];
	cout<<"Enter all elements of array"<<endl;
	for (int i=0;i<2;i++)
	{
		for(int j=0 ; j<2;j++)
		{
		cin>>a[i][j];
	    }
	}
		for (int i=0;i<2;i++)
	{
		for (int j=0;j<2;j++)
	    {
		cout<<a[i][j]<<"\t";
	    }
    }
}
