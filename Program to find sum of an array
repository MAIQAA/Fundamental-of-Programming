#include <iostream>

using namespace std;

int main()
{
   int sum=0;
   
   int marks[5];
   
   for(int i=0;i<5;i++){
       
       cout<<"\nEnter marks["<<i<<"]=";
       cin>>marks[i];
   }
   
   for(int j=0;j<5;j++){
       
       sum+=marks[j];
       
   }
   
   cout<<"\nSum="<<sum;
    return 0;
}
